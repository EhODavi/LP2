import java.util.Scanner;

//objetivo é atingir 100 pontos antes do seu adversário.
class JogoPig {
	public static void main(String[] args) {
		Scanner leia = new Scanner(System.in);
		Pig jogao = new Pig();
		jogao.jogadores[0] = new Jogador();
		jogao.jogadores[1] = new Jogador();
		leia.close();
	}
}
