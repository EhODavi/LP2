
class AlgumaClasse {
	public static void main(String[] args) {
		int[][] idades = new int[3][3];
		for (int i = 0; i < idades.length; i++) {
			for (int j = 0; j < idades[i].length; j++) {
				idades[i][j] = i * 10;
			}
		}
		// imprimindo toda a array
		for (int x : idades) {
			System.out.println(x);
		}
	}
}
