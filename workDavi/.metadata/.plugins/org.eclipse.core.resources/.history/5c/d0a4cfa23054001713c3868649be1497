
class Funcionario {
	private String nome;
	private String departamento;
	private double salario;
	private String dataEntrada;
	private String RG;

	void recebeAumento(double valor) {
		this.salario = this.salario + valor;
	}

	double getGanhoAnual() {
		double valorAnual = this.salario * 12;
		return valorAnual;
	}
}