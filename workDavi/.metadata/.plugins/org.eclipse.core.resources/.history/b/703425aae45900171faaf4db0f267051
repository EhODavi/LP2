
class CorrecaoProvaExercicio2 {
	private double a;
	private double b;
	private double c;
	private static int numTriangulosCriados;

	void instanciar(double a, double b, double c) {
		if (a > 0 && b > 0 && c > 0 && a < b + c && b < a + c && c < a + b) {
			this.a = a;
			this.b = b;
			this.c = c;
		}
	}

	double perimetro() {
		return this.a + this.b + this.c;
	}

	double area() {
		return 0;
	}
}
