package listaExercicios05;

class TestaFornecedorPessoa {
	public static void main(String[] args) {
		Pessoa p = new Pessoa();
		p.setNome("Davi");
		p.setIdade(16);
		p.setCpf("123");
		p.setPeso(60);
		Fornecedor f = new Fornecedor();
		f.setNome("Davi");
		f.setIdade(16);
		f.setCpf("123");
		f.setPeso(60);
		f.setCreditoMax(1000);
		f.setvalorEmDivida(2000);
		p = f; // IMPORTANTE
		
	}
}
