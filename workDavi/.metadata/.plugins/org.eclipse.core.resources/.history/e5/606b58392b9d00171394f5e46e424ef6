package listaExercicios05;

public class SistemaContainer {
	private Container[] conts;
	private int fim;
	private static SistemaContainer instance;

	public SistemaContainer() {
		conts = new Container[60];
	}

	// Singleton
	public static SistemaContainer getInstance() {
		if (instance == null) {
			instance = new SistemaContainer();
		}
		return instance;
	}

	public boolean adiciona(Container c) {
		if (c != null && fim < conts.length) { // Verificando se é possível
												// inicializar
			
		}
	}
}
